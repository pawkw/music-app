{
  "name": "musaic",
  "version": "0.1.0",
  "description": "A simple music player made in Electron.",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-win64": "electron-packager . Musaic --platform=win32 --arch=all --icon=icons/icon.ico --out=release --asar --overwrite",
    "build-linux": "electron-packager . Musaic --platform=linux --arch=x64 --icon=icons/icon.png --out=release --asar --overwrite" ,
    "build-mac": "electron-packager . Musaic --platform=darwin --arch=x64 --icon=icons/icon.icns --out=release --asar --overwrite"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pawkw/music-app.git"
  },
  "keywords": [
    "Electron",
    "Music"
  ],
  "author": "Peter Weston",
  "license": "GPL 3.0",
  "bugs": {
    "url": "https://github.com/pawkw/music-app/issues"
  },
  "homepage": "https://github.com/pawkw/music-app#readme",
  "dependencies": {
    "electron": "^15.5.5",
    "jquery": "^3.4.1",
    "music-metadata": "^4.2.0"
  },
  "devDependencies": {
    "electron-packager": "^14.0.4",
    "eslint": "^6.1.0"
  }
}
